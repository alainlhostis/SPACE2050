@article{bergmannReimaginingGIScienceRelational,
  title = {Reimagining {{GIScience}} for Relational Spaces},
  author = {Bergmann, Luke and O'Sullivan, David},
  journal = {The Canadian Geographer / Le G\'eographe canadien},
  pages = {n/a-n/a},
  issn = {1541-0064},
  doi = {10/gc6wr6},
  url = {http://onlinelibrary.wiley.com/doi/10.1111/cag.12405/abstract},
  urldate = {2017-10-09},
  abstract = {A mismatch between largely absolute Newtonian models of space in GIScience and the relational spaces of critical human geography has contributed to mutual disinterest between the fields. Critical GIS has offered an intellectual critique of GIScience without substantially altering how particular key geographical concepts are expressed in data structures. Although keystone ideas in GIScience such as Tobler's ``First Law'' and the modifiable areal unit problem speak to enduring concerns of human geography, they have drawn little interest from that field. Here, we suggest one way to reformulate the computational approach to the region for relational space, so that regions emerge not through proximity in an absolute space or similarities in intensive properties, but according to their similarities in relations. We show how this might operate theoretically and empirically, working through three illustrative examples. Our approach gestures toward reformulating key terms in GIScience like distance, proximity, networks, and spatial building blocks such as the polygon. Re-engaging the challenges of representing geographical concepts computationally can yield new kinds of GIS and GIScience resonant with theoretical ideas in human geography, and also lead to critical human geographic practices less antagonistic to computation.},
  langid = {english},
  keywords = {critical GIS,distance,espace relationnel,regions,r\'egions,relational space,SIG critique},
  file = {/home/alain/Zotero/storage/U3QWQ64E/Bergmann_O'Sullivan_Reimagining GIScience for relational spaces.pdf;/home/alain/Zotero/storage/DHJAEAA7/abstract.html}
}

