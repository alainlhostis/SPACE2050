@article{currentInteractiveApproachIdentify1990,
  title = {An Interactive Approach to Identify the Best Compromise Solution for Two Objective Shortest Path Problems},
  author = {Current, J.R. and ReVelle, C.S. and Cohon, J.L.},
  year = {1990},
  journal = {Computers \& Operations Research},
  volume = {17},
  number = {2},
  pages = {187--198},
  doi = {10/fjkrvb},
  url = {http://www.sciencedirect.com/science/article/B6WS6-403DWBP-SJ/1/46bf75d4a70d603671fba21d94090414},
  abstract = {n recent years there has been an increase in research on multiobjective shortest path problems. Objectives of interest have included cost, time, delay, accessibility, risk, reliability, and environmental impact, among others. In general, there is no single optimal solution in multiobjective problems but rather, a set of noninferior (Pareto optimal) solutions from which the decision maker must select the most preferred or best compromise solution as the one to implement. Single objective shortest path problems can, in general, be solved extremely efficiently. Unfortunately, the number of noninferior solutions to two objective shortest path problems may grow exponentially with the problem size. Consequently, the generation of the entire noninferior solution set is not practical for most real-world problems. The authors present a method to assist the decision maker in identifying the best compromise solution. The method incorporates interaction between the decision maker and the analyst to reduce the number of noninferior solutions that must be generated in order to identify the solution to be implemented. This interaction permits the decision maker to include additional criteria, values, and personal judgment in the decision making process.}
}

@article{currentMedianShortestPath1987,
  title = {The Median Shortest Path Problem: A Multiobjective Approach to Analyze Cost vs. Accessibility in the Design of Transportation Networks},
  author = {Current, J.R. and Revelle, C.S. and Cohon, J.L.},
  year = {1987},
  journal = {Transportation Science},
  volume = {21},
  number = {3},
  pages = {188--197},
  doi = {10/cr8whh},
  url = {http://www.sciencedirect.com/science/article/B6WS6-4061W3K-55/1/95dc3ad446366df57be42640e3ba784d C:\Travail\Bibliographie\Articles\Horaires correspondances\users behaviour simulation intercity rail Crisalli.pdf},
  abstract = {The authors introduce the median shortest path problem (MSPP). The MSPP is a bicriterion path problem with the objectives being the minimization of the total path length and the minimization of the total travel time required for demand to reach a node on the path. Potential applications of the MSPP include, among others, the location of new highways, railroad lines and subway lines and the design of airline routes. It is particularly applicable in transportation network design problems where the trade-off between operator costs and user costs is important. An algorithm is presented to identify noninferior solutions to the MSPP. This algorithm incorporates a K shortest path algorithm. The algorithm is demonstrated with a sample problem and the results are compared to those obtained using integer programming.}
}

