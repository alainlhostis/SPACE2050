@article{mullerCartographieEspacesFonctionnels1983,
  title = {La {{Cartographie}} Des Espaces Fonctionnels},
  author = {M{\"u}ller, Jean-Claude},
  year = {1983},
  journal = {L'Espace g\'eographique},
  volume = {12},
  number = {2},
  eprint = {44380186},
  eprinttype = {jstor},
  pages = {142--152},
  issn = {0046-2497},
  doi = {10/ggw6cc},
  url = {https://www.jstor.org/stable/44380186},
  urldate = {2019-02-25},
  abstract = {[A la suite de travaux pr\'eliminaires publi\'es r\'ecemment dans cette revue, on sugg\`ere de nouvelles solutions au probl\`eme de la cartographie des distances non-euclidiennes. Les proc\'ed\'es d'\'echelonnage multidimensionnel s'av\`erent insatisfaisants, dans la mesure o\`u ils impliquent une repr\'esentation euclidienne ou minkowskienne en contradiction avec la m\'etrique des donn\'ees. Plut\^ot que d'optimiser la repr\'esentation des donn\'ees sur un fond g\'eom\'etrique connu, on propose de transformer l'\'echelle de la carte pour l'adapter \`a la g\'eom\'etrie (inconnue) des donn\'ees. Quelques exemples appliqu\'es \`a la cartographie des distances-efforts et m\'etaphoriques montrent l'utilit\'e de la m\'ethode pour l'analyse et l'interpr\'etation g\'eographiques. Following preliminary work already published in this journal, new solutions to the problem of mapping non-Euclidean distances are suggested. Multidimensional scaling procedures are unsatisfactory, to the extent that they imply an Euclidean or Minkowskian representation in contradiction with the data metric. Rather than optimazing the representation of the data on a geometrically known surface, we propose to transform the scaling of the map in accordance with the geometry (unknown) of the data. Some examples applied to the mapping of effort and metaphoric distances show the use of the method for geographical analysis and interpretation.]}
}

