@article{coulombelWhyHousingTransport2018,
  title = {Why Housing and Transport Costs Should Always Be Considered Together: {{A}} Monocentric Analysis of Prudential Measures in Housing Access},
  shorttitle = {Why Housing and Transport Costs Should Always Be Considered Together},
  author = {Coulombel, Nicolas},
  year = {2018},
  month = jul,
  journal = {Transport Policy},
  series = {Household Transport Costs, Economic Stress and Energy Vulnerability},
  volume = {65},
  pages = {89--105},
  issn = {0967-070X},
  doi = {10.1016/j.tranpol.2017.04.011},
  url = {https://www.sciencedirect.com/science/article/pii/S0967070X17302871},
  urldate = {2022-03-30},
  abstract = {This paper compares the effects of two prudential measures in housing access on the solvency and welfare of households, with specific attention to transport costs and equity issues. A widespread regulation limits the housing burden (defined as the share of income spent on housing), typically at 33\% or so. Using the monocentric model, I show that capping the housing burden drives low-income households away from the city center toward suburban areas, where they face high transport costs. This unintended eviction effect lowers the prudential efficiency of this measure. Capping the housing plus transport burden precludes this eviction mechanism and better protects household solvency, including from strong variations in transport costs such as during fuel price spikes. Additionally, by limiting the bidding capacity of households, both prudential measures lead to a decrease in housing prices. This tends to improve the welfare of households, firstly of high-income ones (being less affected by the constraints). The effect is stronger when capping the housing burden, so that households, again firstly high-income ones, are typically better off (in terms of welfare) than when one caps the housing plus transport burden, however. Considering the primary objective of prudential measures \textendash{} protecting household solvency, firstly of low-income households \textendash{} these findings call for the inclusion of transport costs within prudential ratios, as well as indicators of housing affordability. This would incidentally raise public awareness with regard to the high costs of private cars, which are often underestimated. A short application to the Paris region corroborates that a policy change from housing only toward comprehensive housing plus transport prudential ratios might significantly improve the situation of low-income households.},
  langid = {english},
  keywords = {Housing affordability,Location efficiency,Monocentric model,Transport costs,Welfare},
  annotation = {16 citations (Semantic Scholar/DOI) [2022-03-30]},
  file = {/home/alain/uLilleCloud/Travail/Bibliographie/zotfile/Coulombel_2018_Why housing and transport costs should always be considered together.pdf}
}

