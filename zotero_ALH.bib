@article{millerFieldBasedTheoryTime2009,
  title = {A {{Field-Based Theory}} for {{Time Geography}}},
  author = {Miller, Harvey J. and Bridwell, Scott A.},
  year = {2009},
  journal = {Annals of the Association of American Geographers},
