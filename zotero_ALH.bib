@article{grahamRelationalConceptsSpace1999,
  title = {Relational {{Concepts}} of {{Space}} and {{Place}}: {{Issues}} for {{Planning Theory}} and {{Practice}}},
  shorttitle = {Relational {{Concepts}} of {{Space}} and {{Place}}},
  author = {Graham, Stephen and Healey, Patsy},
