@article{ahmedTimeSpaceTransformations2007,
  title = {Time\textendash Space Transformations of Geographic Space for Exploring, Analyzing and Visualizing Transportation Systems},
  author = {Ahmed, Nobbir and Miller, Harvey J.},
  year = {2007},
  journal = {Journal of Transport Geography},
  volume = {15},
  number = {1},
  pages = {2--17},
  issn = {0966-6923},
  doi = {10/dbgnd3},
  url = {http://www.sciencedirect.com/science/article/pii/S0966692305000761},
  urldate = {2012-08-16},
  abstract = {Transportation systems exist within at least two types of space. One is the apparent geographic space, but equally important is the time\textendash space implied by the travel time relations created by the system. Differences between the geographic and time\textendash spaces are properties induced by the transportation system. Methods for time\textendash space transformations of geographic space to explore, visualize and analyze transportation systems were initially developed in the 1960s and 1970s. However, these methods have not been pursued beyond this initial flurry of research activity, most likely due to the difficulties associated with handling and processing digital geographic data. The rise of geographic information systems (GIS), as well as continued development and wider availability of transformation techniques such as multidimensional scaling (MDS) and spatial analytical techniques such as bidimensional regression can allow the potential of time\textendash space transformation techniques to be realized. This paper presents a general methodological framework that exploits recent advances in GIS, MDS and spatial analytical techniques. Results from applying these techniques to the Salt Lake City metropolitan area illustrate the power of these techniques to reveal spatial patterns in the travel time relationships induced by a transportation system. The application also addresses fundamental issues in time\textendash space transformations, such as two-dimensional versus three-dimensional solutions, Euclidean versus non-Euclidean solutions and symmetric and asymmetric solutions.},
  keywords = {Bidimensional regression,Cartographic transformations,GIS,Multidimensional scaling,Time\textendash space,Transportation,Travel time},
  annotation = {48 citations (Crossref) [2021-04-20]},
  file = {/home/alain/Zotero/storage/9UT5BC9M/Ahmed et Miller - 2007 - Time–space transformations of geographic space for.pdf;/home/alain/Zotero/storage/6EC7TXA4/S0966692305000761.html}
}

