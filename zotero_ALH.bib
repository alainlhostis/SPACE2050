@article{orfeuilQuellesInteractionsEntre2002,
  title = {Quelles Interactions Entre Les March\'es de La Mobilit\'e \`a Courte et \`a Longue Distance~?},
  author = {Orfeuil, Jean-Pierre and Soleyret, Dani{\`e}le},
  year = {2002},
  month = oct,
  journal = {Recherche - Transports - S\'ecurit\'e},
  volume = {76},
  number = {Supplement C},
  pages = {208--221},
  issn = {0761-8980},
  doi = {10/bpk9qw},
  url = {http://www.sciencedirect.com/science/article/pii/S0761898002000134},
  urldate = {2017-12-13},
  abstract = {R\'esum\'e Cet article propose une approche quantitative globale de la mobilit\'e, int\'egrant les pratiques des personnes sur trois segments~: la mobilit\'e locale de semaine, la mobilit\'e locale de fin de semaine et la mobilit\'e \`a longue distance. Une premi\`ere partie \'evalue globalement ces diff\'erents march\'es et montre le poids croissant de la mobilit\'e \`a longue distance. L'analyse de chacun de ces march\'es, puis de la mobilit\'e globale est conduite ensuite suivant la position dans le cycle de vie, la localisation r\'esidentielle et le niveau de revenu. Enfin, les r\^oles respectifs de la localisation et du revenu dans la formation de la demande sur les diff\'erents march\'es sont \'etudi\'es dans la partie centrale du cycle de vie. On montre notamment l'absence de compensation entre la mobilit\'e de semaine et la mobilit\'e de fin de semaine, mais on note des interactions non n\'egligeables entre la mobilit\'e locale et la mobilit\'e \`a longue distance, notamment avec la localisation r\'esidentielle. Abstract This paper proposes a comprehensive quantitative approach to travel analysis that considers personal travel practices with regard to three travel market segments: local weekday travel, local weekend travel and long-distance travel. The first section consists of an evaluation of the overall size of these segments and demonstrates the increasing importance of long-distance travel. Each market segment and then travel as a whole are then analyzed according to individuals' position in the life cycle, residential location and income level. Last, the roles of location and income in the creation of travel demand in the different segments are then investigated for individuals in the middle part of the life cycle. In particular, it is demonstrated that weekend and weekday travel do not compensate for one another, but there are significant interactions between both local and long-distance travel, and, in particular, residential location.},
  keywords = {Local travel,Long-distance travel,Mobilit\'e \`a longue distance,Mobilit\'e globale,Mobilit\'e locale,Passenger transport,Total travel,Transport de voyageurs},
  file = {/home/alain/Zotero/storage/2TC6MY8Q/Orfeuil et Soleyret - 2002 - Quelles interactions entre les marchés de la mobil.pdf;/home/alain/Zotero/storage/64I38WYI/S0761898002000134.html}
}

