@article{martinPostmathematicalTopology2013,
  title = {Towards a Post-Mathematical Topology},
  author = {Martin, Lauren and Secor, Anna J.},
  year = {2013},
  journal = {Progress in Human Geography},
  pages = {0309132513508209},
  issn = {0309-1325, 1477-0288},
