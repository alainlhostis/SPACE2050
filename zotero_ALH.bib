  keywords = {⛔ No DOI found},
  file = {/home/alain/Zotero/storage/ANSZS3P8/Richer et Vuidel - 2011 - L'intensité nodale, une évaluation de la performan.pdf;/home/alain/Zotero/storage/MVPB3ID3/halshs-00798567.html}
