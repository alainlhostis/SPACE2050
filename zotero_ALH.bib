@article{buehlerMakingPublicTransport2011,
  title = {Making Public Transport Financially Sustainable},
  author = {Buehler, Ralph and Pucher, John},
  year = {2011},
  month = jan,
  journal = {Transport Policy},
  volume = {18},
  number = {1},
