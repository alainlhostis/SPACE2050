@article{crosbyEmbeddingRoadNetworks2019,
  title = {Embedding Road Networks and Travel Time into Distance Metrics for Urban Modelling},
  author = {Crosby, Henry and Damoulas, Theodore and Jarvis, Stephen A.},
  year = {2019},
  month = mar,
  journal = {International Journal of Geographical Information Science},
  volume = {33},
