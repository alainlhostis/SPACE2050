@article{moinseIntermodalUseScooters2022,
  title = {Intermodal Use of (e-)Scooters with Train in the {{Provence-Alpes-C\^ote}} d'{{Azur}} Region: Towards Extended Train Stations Areas?},
  shorttitle = {Intermodal Use of (e-)Scooters with Train in the {{Provence-Alpes-C\^ote}} d'{{Azur}} Region},
  author = {Moinse, Dylan and Goudeau, Matthieu and L'Hostis, Alain and Leysens, Thomas},
  year = {2022},
