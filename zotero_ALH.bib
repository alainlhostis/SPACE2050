  booktitle = {Proceedings of the 2018 {{CHI Conference}} on {{Human Factors}} in {{Computing Systems}}},
  author = {Hong, Sungsoo (Ray) and Yoo, Min-Joon and Chinh, Bonnie and Han, Amy and Battersby, Sarah and Kim, Juho},
