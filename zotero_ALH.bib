@article{dablancRiseOndemandInstant2017,
  title = {The Rise of On-Demand `{{Instant Deliveries}}' in {{European}} Cities},
  author = {Dablanc, Laetitia and Morganti, Eleonora and Arvidsson, Niklas and Woxenius, Johan and Browne, Michael and Saidi, Ne{\"i}la},
  year = {2017},
  month = oct,
  journal = {Supply Chain Forum: An International Journal},
  volume = {18},
  number = {4},
  pages = {203--217},
  issn = {1625-8312},
  doi = {10/gcp3kn},
  url = {https://doi.org/10.1080/16258312.2017.1375375},
  urldate = {2019-07-12},
  abstract = {This exploratory paper contributes to a new body of research that investigates the potential of digital market places to disrupt transport and mobility services. We are specifically looking at the urban freight sector, where numerous app-based services have emerged in recent years. The paper specifically looks at `instant deliveries,' i.e. services providing on-demand delivery within two hours \textendash{} by either private individuals, independent contractors, or employees \textendash{} by connecting consignors, couriers and consignees via a digital platform. The paper provides an overview of the main issues concerning instant deliveries, supported by data (including a survey of 96 courier delivery providers) and examples. After presenting a typology of companies (digital platforms) involved in `instant deliveries,' we question in what way they transform the urban freight current patterns. We highlight four issues, discussing their potential to impact urban freight services and related policies in European cities: 1) Freight trips and data; 2) Business models; 3) Labor legislation and work conditions; and 4) Local public policies. We conclude by saying that predicting the medium-term consequences of these changes is difficult, but it is essential that city planning and policies take account of these developments and consider how planning and possibly regulation needs to be adapted to these new ways of doing things.},
  keywords = {crowd shipping,crowd sourcing,Instant delivery,on-demand delivery,urban freight,urban logistics},
  file = {/home/alain/Zotero/storage/CW4NGT7K/Dablanc et al. - 2017 - The rise of on-demand ‘Instant Deliveries’ in Euro.pdf;/home/alain/Zotero/storage/E9TPZV7H/16258312.2017.html}
